{
  "name": "pos-system",
  "version": "1.0.0",
  "description": "electron-react-redux-webpack-electron",
  "main": "main.js",
  "productName": "PosSystem",
  "homepage": "./",
  "repository": "https://github.com/rio1228/pos-electron.git",
  "scripts": {
    "start": "electron .",
    "build": "webpack",
    "build-w": "webpack --watch",
    "component": "asar pack ./out ./project.asar",
    "all-package": "webpack && asar pack ./out ./project.asar && electron-packager . pos-system --platform=win32 --arch=x64 --prune=true  --out=release-builds",
    "all-repackage": "webpack && asar pack ./out ./project.asar && electron-packager . pos-system --platform=win32 --arch=x64 --prune=true  --out=release-builds --overwrite",
    "package": "electron-packager ./ pos-system --platform=win32 --arch=x64 --prune=true  --out=release-builds",
    "repackage": "electron-packager . pos-system --platform=win32 --arc=x64 --prune=true  --out=release-builds --overwrite"
  },
  "keywords": [],
  "author": "Ryo Mikami",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.17.2",
    "consolidate": "^0.14.5",
    "cookie-parser": "^1.4.3",
    "electron": "^1.7.5",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "material-ui": "^0.19.0",
    "multer": "^1.3.0",
    "mustache": "^2.3.0",
    "mysql": "^2.14.1",
    "photon": "git+https://github.com/connors/photon.git",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.2",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "serve-static": "^1.12.4",
    "superagent": "^3.5.2",
    "webpack": "^3.5.2"
  },
  "devDependencies": {
    "electron-packager": "^8.7.2",
    "morgan": "^1.8.2",
    "webpack-dev-server": "^2.7.1"
  }
}
